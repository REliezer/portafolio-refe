---
import type { SkillEntry } from "@/types";
import BaseLayout from "@components/base/BaseLayout.astro";
//import TechGraph from "@components/skill/TechGraph";
import TechTable from "@components/skill/TechTable.astro";
import PageHeader from "@components/common/PageHeader.astro";
import { getIndex } from "@lib/contentParser";

const entry = (await getIndex("skill")) as SkillEntry;

const techs = entry.data.techs;
---

<BaseLayout title={entry.data.title} description={entry.data.description}>
  <PageHeader title={entry.data.title} />
  {/*<TechGraph client:load techs={techs}/>*/}
  <TechTable
    title={entry?.data?.title}
    description={entry?.data?.description}
    techs={techs}
  />
</BaseLayout>
